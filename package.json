{
  "name": "strata-vote",
  "version": "1.0.0",
  "description": "Production-ready strata council voting system with one-time links",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "test": "bash smoke-test.sh",
    "docker:build": "docker build -t stratavote .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "backup": "bash scripts/backup.sh"
  },
  "keywords": [
    "voting",
    "strata",
    "council"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "better-sqlite3-session-store": "^0.1.0",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "nodemailer": "^6.9.0",
    "pdfkit": "^0.14.0",
    "winston": "^3.11.0"
  }
}
